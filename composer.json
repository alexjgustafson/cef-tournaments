{
    "name": "alexjgustafson/cef-tournaments",
    "description": "Tools for displaying chess tournaments",
    "type": "wordpress-plugin",
    "repositories": [
        {
            "type": "composer",
            "url": "https://connect.advancedcustomfields.com"
        },
        {
            "type":"composer",
            "url":"https://wpackagist.org",
            "only": [
                "wpackagist-plugin/*",
                "wpackagist-theme/*"
            ]
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "illuminate/collections": "^8.83",
        "wpengine/advanced-custom-fields-pro": "*",
        "guzzlehttp/guzzle": "^7.10",
        "spatie/ray": "^1.43"
    },
    "autoload": {
        "psr-4": {
            "CEF\\Tournaments\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Alex J. Gustafson",
            "email": "alexjgustafson@gmail.com"
        }
    ],
    "config": {
        "platform": {
            "php": "7.4.33"
        },
        "allow-plugins": {
            "composer/installers": true,
            "roots/wordpress-core-installer": true
        }
    },
    "version": "1.0.0",
    "extra": {
        "installer-paths": {
            "vendor/{$name}/": ["wpengine/advanced-custom-fields-pro"],
            "wordpress/wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
            "wordpress/wp-content/themes/{$name}/": ["type:wordpress-theme"]
        }
    },
    "require-dev": {
        "roots/wordpress": "^6.8"
    }
}
